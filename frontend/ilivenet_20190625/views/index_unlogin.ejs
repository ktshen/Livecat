                                                        </div>
                                                    <% } %>
                                                    <!-- live status -->
                                                    <% if (post.status == "live" || post.status == "Live" || post.status == "LIVE") { %>
                                                        <div class="rounded livestatus">
                                                            <a style="color: #FFFFFF">LIVE</a>
                                                        </div>
                                                    <% } %>
                                                </div>
                                                <div>
                                                    <div>
                                                        <h6 class="text-black omit" style="color: #3e5569"><%= post.title %></h6>
                                                    </div>
                                                    <p class="text-black-50 "><%= post.host %></p>
                                                </div>
                                            </div>
                                        </a>
                                    </div>
                                </div>
                            <% } %>
                        <%}) %>
                    </div>
                <% } %>
                <br>
                <div class="d-flex justify-content-center">
